<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:orientation="vertical"
    android:layout_width="829px"
    android:layout_height="723px"
    >
    <LinearLayout
        android:layout_width="829px"
        android:layout_height="38px"
        android:orientation="horizontal"
        >
        <TextView
            android:id="@+id/history_all"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_weight="1"
            android:gravity="center"
            android:text="전체"
            android:textSize="15px"
            android:textColor="#ffdf5f"
            android:background="#4c4c4c"
            />
        <TextView
            android:id="@+id/history_in"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_weight="1"
            android:gravity="center"
            android:text="입차중"
            android:textSize="15px"
            android:textColor="#ffdf5f"
            android:background="#4c4c4c"
            />
        <TextView
            android:id="@+id/history_out"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_weight="1"
            android:gravity="center"
            android:text="정상출차"
            android:textSize="15px"
            android:textColor="#ffdf5f"
            android:background="#4c4c4c"
            />
        <TextView
            android:id="@+id/history_no_pay"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_weight="1"
            android:gravity="center"
            android:text="미결제"
            android:textSize="15px"
            android:textColor="#ffdf5f"
            android:background="#4c4c4c"
            />
        <TextView
            android:id="@+id/history_cancel"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_weight="1"
            android:gravity="center"
            android:text="입차취소"
            android:textSize="15px"
            android:textColor="#ffdf5f"
            android:background="#4c4c4c"
            />
    </LinearLayout>
    <LinearLayout
        android:layout_width="829px"
        android:layout_height="87px"
        android:orientation="horizontal"
        >
        <EditText
            android:id="@+id/search_car_num"
            android:layout_width="460px"
            android:layout_height="wrap_content"
            android:hint="차량 번호 검색"
            android:inputType="number"
            android:gravity="center_vertical|center"
            android:layout_gravity="center"
            android:layout_marginLeft="184px"
            android:textSize="16px"
            android:textColor="@android:color/black"
            android:textStyle="bold"
            />
        </LinearLayout>
    <LinearLayout
        android:layout_width="829px"
        android:layout_height="38px"
        android:orientation="horizontal"
        android:background="#e3e3e3"
        >
        <TextView
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_weight="1"
            android:text="차량번호"
            android:gravity="center"
            android:textColor="@android:color/black"
            android:textSize="16px"
            />
        <TextView
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_weight="1"
            android:text="입차시간"
            android:gravity="center"
            android:textColor="@android:color/black"
            android:textSize="16px"
            />
        <TextView
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_weight="1"
            android:text="출차시간"
            android:gravity="center"
            android:textColor="@android:color/black"
            android:textSize="16px"
            />
        <TextView
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_weight="1"
            android:text="이용요금"
            android:gravity="center"
            android:textColor="@android:color/black"
            android:textSize="16px"
            />
        <TextView
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_weight="1"
            android:text="할인금액"
            android:gravity="center"
            android:textColor="@android:color/black"
            android:textSize="16px"
            />
        <TextView
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_weight="1"
            android:text="결제금액"
            android:gravity="center"
            android:textColor="@android:color/black"
            android:textSize="16px"
            />
        <TextView
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_weight="1"
            android:text="입차"
            android:gravity="center"
            android:textColor="@android:color/black"
            android:textSize="16px"
            />
        <TextView
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_weight="1"
            android:text="결제"
            android:gravity="center"
            android:textColor="@android:color/black"
            android:textSize="16px"
            />
    </LinearLayout>
    <ListView
        android:id="@+id/history_list"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        />
</LinearLayout>