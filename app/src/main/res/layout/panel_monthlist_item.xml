<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:orientation="vertical"
    android:layout_width="182px"
    android:layout_height="156px"
    android:background="@drawable/panel_monthlist_item_shape"
    >
    <TextView
        android:id="@+id/car_num"
        android:layout_width="182px"
        android:layout_height="62px"
        android:gravity="center"
        android:text="번호"
        android:textColor="@android:color/white"
        android:textSize="24px"
        />
    <LinearLayout
        android:layout_width="182px"
        android:layout_height="24px"
        android:orientation="horizontal"
        android:gravity="center"
        >
        <TextView
            android:id="@+id/car_name"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="산타페"
            android:textSize="16px"
            android:textColor="@android:color/white"
            />
        <TextView
            android:id="@+id/car_type_title"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="중형"
            android:textSize="16px"
            android:textColor="@android:color/white"
            />
    </LinearLayout>
    <LinearLayout
        android:layout_width="182px"
        android:layout_height="16px"
        android:paddingLeft="11px"
        android:orientation="horizontal"
        >
        <TextView
            android:id="@+id/start_date"
            android:layout_width="wrap_content"
            android:layout_height="16px"
            android:gravity="center_vertical"
            android:text="0000-00-00 ~"
            android:textSize="12px"
            android:textColor="@android:color/white"
            />
        <TextView
            android:id="@+id/end_date"
            android:layout_width="wrap_content"
            android:layout_height="16px"
            android:gravity="center_vertical"
            android:text=" 0000-00-00"
            android:textSize="12px"
            android:textColor="@android:color/white"
            />
    </LinearLayout>
    <LinearLayout
        android:layout_width="182px"
        android:layout_height="16px"
        android:orientation="horizontal"
        android:paddingLeft="11px"
        >
        <TextView
            android:layout_width="54px"
            android:layout_height="16px"
            android:gravity="center_vertical"
            android:text="월차 금액"
            android:textSize="12px"
            android:textColor="@android:color/white"
            />
        <TextView
            android:id="@+id/amount"
            android:layout_width="wrap_content"
            android:layout_height="16px"
            android:gravity="center_vertical"
            android:text="0"
            android:textSize="12px"
            android:textColor="@android:color/white"
            />
    </LinearLayout>
    <LinearLayout
        android:layout_width="182px"
        android:layout_height="16px"
        android:orientation="horizontal"
        android:paddingLeft="11px"
        android:layout_marginBottom="22px"
        >
        <TextView
            android:id="@+id/user_name"
            android:layout_width="54px"
            android:layout_height="16px"
            android:gravity="center_vertical"
            android:text="홍길동"
            android:textSize="12px"
            android:textColor="@android:color/white"
            />
        <TextView
            android:id="@+id/mobile"
            android:layout_width="wrap_content"
            android:layout_height="16px"
            android:gravity="center_vertical"
            android:text="010-0000-0000"
            android:textSize="12px"
            android:textColor="@android:color/white"
            />
    </LinearLayout>
</LinearLayout>